    </context-param>
    <context-param>
        <param-name>QDBPOOL_JDBC_CLASS</param-name>
        <param-value>org.postgresql.Driver</param-value>
    </context-param>
    <context-param>
        <param-name>debug</param-name>
        <param-value>true</param-value>
    </context-param>
</web-app>