{
  "name": "request-handler-front-angular",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "style": "scss"
    }
  },
  "sourceRoot": "apps/request-handler-front-angular/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/request-handler-front-angular",
        "index": "apps/request-handler-front-angular/src/index.html",
        "main": "apps/request-handler-front-angular/src/main.ts",
        "baseHref": "/dispatcher/",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/request-handler-front-angular/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/request-handler-front-angular/src/favicon.ico",
          "apps/request-handler-front-angular/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/deeppurple-amber.css",
          "node_modules/bootstrap/scss/bootstrap.scss",
          "apps/request-handler-front-angular/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/request-handler-front-angular/src/environments/environment.ts",
              "with": "apps/request-handler-front-angular/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "stage": {
          "fileReplacements": [
            {
              "replace": "apps/request-handler-front-angular/src/environments/environment.ts",
              "with": "apps/request-handler-front-angular/src/environments/environment.stage.ts"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "request-handler-front-angular:build:production"
        },
        "development": {
          "browserTarget": "request-handler-front-angular:build:development",
          "proxyConfig": "./apps/request-handler-front-angular/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "request-handler-front-angular:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": ["zone.js", "zone.js/testing"],
        "tsConfig": "apps/request-handler-front-angular/tsconfig.spec.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/request-handler-front-angular/src/favicon.ico",
          "apps/request-handler-front-angular/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/deeppurple-amber.css",
          "node_modules/bootstrap/scss/bootstrap.scss",
          "apps/request-handler-front-angular/src/styles.scss"
        ],
        "scripts": []
      }
    }
  }
}
